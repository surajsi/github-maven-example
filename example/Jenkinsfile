pipeline {
		agent any
	
	tools {
		maven 'maven'}
		
		stages {
			stage ('clean') {
			steps {
								
				
				sh 'mvn -f example/pom.xml clean'
				
				}
				}
			stage ('package') {
			steps {
				
				sh 'mvn -f example/pom.xml package'	
				
				}
				}
			stage ('mail sending'){
				steps {
					emailext attachLog: true, body: 'hello', recipientProviders: [requestor()], subject: 'job status', to: 'surajsinghjuly27@gmail.com'
				}
			}
				}
				}
